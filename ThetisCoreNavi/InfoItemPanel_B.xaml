    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="50" Width="267" IsTabStop="True" Focusable="False" IsHitTestVisible="True">
    <Grid x:Uid="Grid_1" HorizontalAlignment="Stretch">
        <Grid.ColumnDefinitions>
            <ColumnDefinition x:Uid="ColumnDefinition_1" Width="200*" />
            <ColumnDefinition x:Uid="colImg" Name="colImg" Width="50" />
        </Grid.ColumnDefinitions>
        <TextBox x:Uid="txbItem" Grid.Column="0" Name="txbItem" Background="Transparent" TextWrapping="Wrap" BorderBrush="Transparent" Foreground="White" Focusable="False" IsHitTestVisible="False" IsTabStop="False" IsUndoEnabled="False" IsReadOnly="True" />
        <Image x:Uid="imgThumb" Margin="0,9.5,5,5.5" Name="imgThumb" Grid.Column="1" />
    </Grid>
</UserControl>
