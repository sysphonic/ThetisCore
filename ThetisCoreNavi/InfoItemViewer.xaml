<UserControl x:Uid="UserControl_1" x:Class="ThetisCore.Navi.InfoItemViewer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="208" Width="244">
    <Grid x:Uid="Grid_1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Grid.RowDefinitions>
            <RowDefinition x:Uid="RowDefinition_1" Height="30" />
            <RowDefinition x:Uid="grdRowImg" Name="grdRowImg" Height="115" />
            <RowDefinition x:Uid="RowDefinition_2" Height="73.312*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition x:Uid="ColumnDefinition_1" Width="200*" />
        </Grid.ColumnDefinitions>
        <StackPanel x:Uid="stpBasicInfo" Name="stpBasicInfo" Orientation="Horizontal" />
        <Image x:Uid="imgThumb" Grid.Row="1" Name="imgThumb" Margin="10,10,122.666,5" Width="150" Height="100" HorizontalAlignment="Left" VerticalAlignment="Center" />
        <FlowDocumentScrollViewer x:Uid="scvContent" Name="scvContent" Margin="0,0,0,-2" Grid.Row="2">
        </FlowDocumentScrollViewer>
    </Grid>
</UserControl>
