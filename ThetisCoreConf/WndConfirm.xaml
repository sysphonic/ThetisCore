<Window x:Uid="Window_1" x:Class="ThetisCore.Conf.WndConfirm"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="ThetisCore" Height="248" Width="349" ShowInTaskbar="False" ResizeMode="NoResize" Topmost="True" WindowStartupLocation="CenterOwner">
    <Grid x:Uid="Grid_1">
        <Label x:Uid="label1" Height="39" Margin="27,19,66,0" Name="label1" VerticalAlignment="Top" FontSize="18" Content="{Binding Source={StaticResource resources}, Path=CONNECTION_RESULT_OK}" />
        <Label x:Uid="label2" Margin="19,60,55,0" Name="label2" Height="30" VerticalAlignment="Top" Content="{Binding Source={StaticResource resources}, Path=CONFIRM_ADOPT_AS_TITLE}" />
        <TextBox x:Uid="tbxTitle" Margin="19,98,15,90" Name="tbxTitle" Height="26" />
        <StackPanel x:Uid="stackPanel1" Height="40" Margin="42,0,41,17" Name="stackPanel1" VerticalAlignment="Bottom" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button x:Uid="btnYes" Height="27" Margin="5,0,5,0" Name="btnYes" Click="btnYes_Click" HorizontalAlignment="Left" Width="80" MaxWidth="80" IsDefault="True">Yes</Button>
            <Button x:Uid="btnNo" Height="27" Margin="5,0,5,0" Name="btnNo" HorizontalAlignment="Left" Width="80" Click="btnNo_Click" MaxWidth="80" IsCancel="True">No</Button>
        </StackPanel>
    </Grid>
</Window>
